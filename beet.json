{
    "name": "factorio datapack",
    "require": [
        "bolt",
        "bolt_expressions"
    ],
    "data_pack": {
        "load": ["src"],
        "pack_format": 22
    },
    "pipeline": ["mecha"],
    "output": "build",
    "meta": {
        "bolt": {
            "entrypoint": [
                "func:main",
                "func:item"
            ]
        }
    }
}